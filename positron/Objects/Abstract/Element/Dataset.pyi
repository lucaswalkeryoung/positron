# -------------------------------------------------------------------------------------------------
# --------------------------- TYPING :: The Dataset Component of Element --------------------------
# -------------------------------------------------------------------------------------------------
from .. Object.Object import Object

from typing import Iterator
from typing import ItemsView
from typing import KeysView
from typing import ValuesView
from typing import Mapping
from typing import Hashable
from typing import NoReturn

T_PRIMITIVE = None | bool | int | float | str


# -------------------------------------------------------------------------------------------------
# ---------------------------------------- CLASS :: Dataset ---------------------------------------
# -------------------------------------------------------------------------------------------------
class Dataset(Object, Mapping[str, T_PRIMITIVE]):

	# ------------------------------------------------------------------------------------------
	# ------------------------------- CONSTRUCTOR :: Constructor -------------------------------
	# ------------------------------------------------------------------------------------------
	def __init__(self, secret: object) -> None:
		...


	# ------------------------------------------------------------------------------------------
	# ----------------------------- OPERATORS :: Iterable Protocol -----------------------------
	# ------------------------------------------------------------------------------------------
	def __contains__(self, item: object) -> bool:
		...


	def __iter__(self) -> Iterator[str]:
		...


	def __bool__(self) -> bool:
		...


	def __len__(self)  -> int:
		...


	# ------------------------------------------------------------------------------------------
	# ----------------------------- OPERATORS :: Mapping Protocol ------------------------------
	# ------------------------------------------------------------------------------------------
	def items(self) -> ItemsView[str, T_PRIMITIVE]:
		...

	def keys(self)  -> KeysView[str]:
		...

	def values(self) -> ValuesView[T_PRIMITIVE]:
		...


	# ------------------------------------------------------------------------------------------
	# ------------------------------ OPERATORS :: Element Access -------------------------------
	# ------------------------------------------------------------------------------------------
	def __getitem__(self, key: Hashable) -> T_PRIMITIVE:
		...


	# ------------------------------------------------------------------------------------------
	# -------------------------- OPERATORS :: Prohibited Modification --------------------------
	# ------------------------------------------------------------------------------------------
	def __setitem__(self, key: Hashable, value: object) -> NoReturn:
		...

	def __delitem__(self, key: Hashable) -> NoReturn:
		...

