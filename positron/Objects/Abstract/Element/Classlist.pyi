# -------------------------------------------------------------------------------------------------
# -------------------------- TYPING :: The Classlist Component of Element -------------------------
# -------------------------------------------------------------------------------------------------
from .. Object.Object import Object

from typing import Sequence
from typing import Iterator
from typing import overload


# -------------------------------------------------------------------------------------------------
# --------------------------------------- CLASS :: Classlist --------------------------------------
# -------------------------------------------------------------------------------------------------
class Classlist(Object, Sequence[str]):

	# ------------------------------------------------------------------------------------------
	# ------------------------------- CONSTRUCTOR :: Constructor -------------------------------
	# ------------------------------------------------------------------------------------------
	def __init__(self, secret: object) -> None:
		...


	# ------------------------------------------------------------------------------------------
	# ----------------------------- OPERATORS :: Iterable Protocol -----------------------------
	# ------------------------------------------------------------------------------------------
	def __contains__(self, item: object) -> bool:
		...


	def __iter__(self) -> Iterator[str]:
		...


	def __bool__(self) -> bool:
		...


	def __len__(self)  -> int:
		...


	# ------------------------------------------------------------------------------------------
	# ------------------------------ OPERATORS :: Element Access -------------------------------
	# ------------------------------------------------------------------------------------------
	@overload
	def __getitem__(self, index: slice) -> list[str]:
		...


	@overload
	def __getitem__(self, index: int) -> str:
		...


	def __getitem__(self, index: int | slice) -> list[str] | str:
		...


	# ------------------------------------------------------------------------------------------
	# -------------------------- OPERATORS :: Prohibited Modification --------------------------
	# ------------------------------------------------------------------------------------------
	def __setitem__(self, index: int | slice, item: object) -> NoReturn:
		...

	def __delitem__(self, index: int | slice) -> NoReturn:
		...